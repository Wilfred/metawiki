{
  "name": "metawiki",
  "version": "0.3.0",
  "description": "A wiki that can be edited from the browser",
  "repository": "https://github.com/Wilfred/metawiki",
  "main": "server.js",
  "keywords": [
    "wiki"
  ],
  "author": "Wilfred Hughes me@wilfred.me.uk",
  "license": "AGPL-3.0",
  "dependencies": {
    "async": "^2.1.0",
    "backbone": "^1.1.2",
    "bunyan": "^1.3.4",
    "bunyan-prettystream": "^0.1.3",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "codemirror": "^5.1.0",
    "coveralls": "*",
    "eslint": "^3.9.0",
    "handlebars": "^4.0.5",
    "istanbul": "^0.4.2",
    "jquery": "3.1.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "1.2.0",
    "mongoose": "^4.4.4",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^5.2.16",
    "push.js": "^0.0.12",
    "requirejs": "^2.1.22",
    "restify": "^4.0.4",
    "stylefmt": "^5.3.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "start": "node src/backend/main.js",
    "load-db": "node src/backend/load-db.js",
    "dump-db": "node src/backend/dump-db.js",
    "drop-db": "node src/backend/drop-db.js",
    "lint-backend": "eslint src/backend",
    "format-backend": "eslint src/backend --fix --no-eslintrc -c .eslintrc-format.json",
    "test": "mocha -R dot src/backend/test",
    "test-with-coverage": "cd src/backend && istanbul cover _mocha --report lcovonly -- -R spec"
